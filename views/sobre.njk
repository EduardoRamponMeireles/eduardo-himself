{% extends "layout.njk" %}

{% block content%}

    <div id="wrapper">
        <img src="{{ info.avatar_url }}" alt="Imagem de {{ info.name }}">
        <h1>{{ info.nome }}</h1>
        <h2>{{ info.bio }}</h2>
    </div>

    <div id="links-footer" class="links">
        {% for link in info.links%}
        <a href="{{ link.url }}" target="_blank">{{ link.name }}</a>
        {% endfor%}
    </div>

{% endblock %}